{
  "extends": [
    "next/core-web-vitals",
    "standard"
  ],
  "rules": {
    "max-len": ["error", {
      "code": 80,
      "ignoreComments": true,
      "ignoreTrailingComments": true,
      "ignoreUrls": true,
      "ignoreStrings": true,
      "ignoreTemplateLiterals": true,
      "ignoreRegExpLiterals": true
    }]
  }
}
